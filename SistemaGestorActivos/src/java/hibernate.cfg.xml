<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
  <session-factory>
    <property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
    <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
    <property name="hibernate.connection.url">jdbc:mysql://localhost:3306/sistemagestoractivos</property>
    <property name="hibernate.connection.username">root</property>
    <property name="hibernate.connection.password">root</property>
    <mapping resource="SistemaGestorActivos/Logic/Puesto.hbm.xml"/>
    <mapping resource="SistemaGestorActivos/Logic/Usuario.hbm.xml"/>
    <mapping resource="SistemaGestorActivos/Logic/Solicitud.hbm.xml"/>
    <mapping resource="SistemaGestorActivos/Logic/Funcionario.hbm.xml"/>
    <mapping resource="SistemaGestorActivos/Logic/Erazon.hbm.xml"/>
    <mapping resource="SistemaGestorActivos/Logic/Activo.hbm.xml"/>
    <mapping resource="SistemaGestorActivos/Logic/Dependencia.hbm.xml"/>
    <mapping resource="SistemaGestorActivos/Logic/Estado.hbm.xml"/>
    <mapping resource="SistemaGestorActivos/Logic/Bien.hbm.xml"/>
    <mapping resource="SistemaGestorActivos/Logic/Categoria.hbm.xml"/>
    <mapping resource="SistemaGestorActivos/Logic/Rol.hbm.xml"/>
  </session-factory>
</hibernate-configuration>
